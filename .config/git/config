[commit]
	gpgSign = true

[core]
	pager = delta

[delta]
  navigate = true
	dark = true

[filter "lfs"]
	clean = "git-lfs clean -- %f"
	process = "git-lfs filter-process"
	required = true
	smudge = "git-lfs smudge -- %f"

[gpg]
	program = gpg

[interactive]
	diffFilter = delta --color-only

[tag]
	gpgSign = true

[merge]
    conflictstyle = zdiff3

